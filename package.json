{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning express js, node js and mongo db",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "start": "node server.js",
